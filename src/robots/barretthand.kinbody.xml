<KinBody name="BarrettHand">
  <Body name="handbase" type="dynamic">
    <Geom type="trimesh" modifiable="false">
      <Translation>0.0  0.0  -0.06</Translation>
      <data>models/barrett/palm.iv 1</data>
      <Render>models/barrett/palm.iv 1</Render>
