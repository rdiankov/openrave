      <data>models/barrett/link1.iv 0.001</data>
      <Render>models/barrett/link1.iv 0.001</Render>
    </Geom>
    <mass type="custom">
      <!-- cylinder -->
      <total>1.1</total>
      <com>0 0 0.04796</com>
      <inertia>0.000556875 0 0 0 0.000556875 0 0 0 0.00111375</inertia>
    </mass>
  </Body>

  <!-- finger 0-->
